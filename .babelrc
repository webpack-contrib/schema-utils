{
  "presets": [
    [
      "env",
      {
        "useBuiltIns": true,
        "targets": {
          "node": "4"
        },
        "exclude": [
          "transform-async-to-generator",
          "transform-regenerator"
        ]
      }
    ],
    [
      "env",
      {
        "useBuiltIns": true,
        "targets": {
          "node": "6.9.0"
        },
        "exclude": [
          "transform-async-to-generator",
          "transform-regenerator"
        ]
      }
    ]
  ],
  "plugins": [
    [
      "transform-object-rest-spread",
      {
        "useBuiltIns": true
      }
    ],
    [
      "babel-plugin-transform-builtin-extend", {
        "globals": ["Error"]
      }
    ]
  ],
  "env": {
    "test": {
      "presets": [
        "env"
      ],
      "plugins": [
        "transform-object-rest-spread",
        [
          "babel-plugin-transform-builtin-extend", {
            "globals": ["Error"]
          }
        ]
      ]
    }
  }
}
